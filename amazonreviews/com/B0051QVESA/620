var cv_mo2KVSDXYXLOSXVso;	   


function cv_mo2KVSDXYXLOSXVloadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes) {

  try {
      cv_mo2KVSDXYXLOSXVso = new SWFObject(swfLocation, "cv_mo2KVSDXYXLOSXVAMPlayerProd", swfWidth, swfHeight, flashVersion, bgColor);	   

      for (var v in swfParams) {
        cv_mo2KVSDXYXLOSXVso.addVariable(v, encodeURIComponent(swfParams[v]));
      }
      for (var v in flashParams) {
        cv_mo2KVSDXYXLOSXVso.addParam(v, flashParams[v]);
      }
      for (var v in embedAttributes) {
        cv_mo2KVSDXYXLOSXVso.setAttribute(v, embedAttributes[v]);
      }

      cv_mo2KVSDXYXLOSXVso.addVariable("swfEmbedTime", new Date().getTime());

      var agt = navigator.userAgent;
       
      var reFirefox = new RegExp("firefox/", "i");
      var regx = agt.split(reFirefox);
      var ffVersion = 8;
      if (regx[1]) {
          var pts = regx[1].split(/[.]/);
          ffVersion = parseFloat(pts[0] + "." + pts[1]);
      }
      var minorVersion = parseFloat(navigator.ap);
      if ((navigator.appVersion.indexOf("Mac")!=-1) || (ffVersion < 1.5)) {
          cv_mo2KVSDXYXLOSXVso.setAttribute("height", swfHeight);
          cv_mo2KVSDXYXLOSXVso.setAttribute("width", swfWidth);
          cv_mo2KVSDXYXLOSXVso.addVariable("oldFirefox", "1");	     
      }

      if (cv_mo2KVSDXYXLOSXVso.write("cv_mo2KVSDXYXLOSXVflashcontent")) {
          fp_resizePlayerSpace('cv_mo2KVSDXYXLOSXV', 
                               320, 
                               283);
      }
      else {
          var nsPrefix = 'cv_mo2KVSDXYXLOSXV';

          fp_resizePlayerSpace(nsPrefix, 0, 0);
          fp_rewriteDiv(nsPrefix, "flashcontent", '<br/>To view this video download <a target="_blank" href="http://www.macromedia.com/go/getflashplayer" target="_top">Flash Player</a> (version 9.0.115 or higher)');
      }
  } catch(err) {
      fp_resizePlayerSpace('cv_mo2KVSDXYXLOSXV', 0, 0);
  }
  return false;
}



function _cv_mo2KVSDXYXLOSXVloadSwf(movieID, playerVersion) {
